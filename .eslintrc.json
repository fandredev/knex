{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier":"error",
      "quotes":"off",
      "semi": "off",
      "comma-dangle":"off",
      "space-before-function-paren":"off"
    }
}
